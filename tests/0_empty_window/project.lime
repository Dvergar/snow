<?xml version="1.0" encoding="utf-8"?>

<project library="lumen">

    <app main="Main" path="bin" file="lumen_empty" />

    <source path="src" />

    <meta title="lumen empty" package="com.lumenframework.samples" version="1.0.0" company="lumenframework" />

    <window  width="320" height="200" orientation="landscape" fps="60" vsync="true" resizable="true" />
    <window fullscreen="true" borderless="true" if="mobile"/>

    <assets path="assets" rename="assets" exclude="*.svg" />
    <assets path="config.json" />

        <!-- This is ONLY required for the tests to build on the automated server -->
        <!-- usually, you just <haxelib name="lumen" /> and that's all -->
    <include name="../../"/>

    <config:lumen>
        <window width="20"/>
    </config:lumen>

</project>
