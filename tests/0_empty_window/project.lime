<?xml version="1.0" encoding="utf-8"?>

<project library="snow">

    <app main="Main" path="bin" file="snow_empty" />

    <source path="src" />

    <meta title="snow empty" package="com.snowframework.samples" version="1.0.0" company="snowframework" />

    <window  width="320" height="200" orientation="landscape" fps="60" vsync="true" resizable="true" />
    <window fullscreen="true" borderless="true" if="mobile"/>

    <assets path="assets" rename="assets" exclude="*.svg" />
    <assets path="config.json" />

        <!-- This is ONLY required for the tests to build on the automated server -->
        <!-- usually, you just <haxelib name="snow" /> and that's all -->
    <include name="../../"/>

    <config:snow>
        <window width="20"/>
    </config:snow>

</project>
