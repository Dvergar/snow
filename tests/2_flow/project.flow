{

  project : {
    name : 'test_flow',
    version : '1.0.0',
    author : 'snowkit',

    app : {
      main : 'Main',
      output : 'bin',
      name : 'flow_test',
      title : 'flow test application',
      package : 'org.snowkit.flow',
      codepaths : [ 'src' ],

    },

    build : {
      dependencies : {
        snow : '*',
      },
      defines : ['one=1', 'two = 2', 'some_define']
    },

    files : {
      config : 'config.json',
      assets : 'assets/'
    }

  },

  snow : {
    window : {
      title:'hello flow'
    }
  },

  if: {
    "mac" : {
      build: {
        defines : ['ismac']
      }
    },
    "!mac && some_define" : {
      build: {
        defines : ['complex']
      }
    }
  }

}